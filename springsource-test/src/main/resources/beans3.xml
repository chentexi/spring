<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="person" class="com.trent.bean.Person">
		<!--<property name="name" value="dd"></property>-->
		<property name="dept" ref="dept"></property>
		<property name="dept.deptName" value="5656565"/>
		<property name="dept.deptNo" value="123132121"/>
		<constructor-arg value="dd"></constructor-arg>
	</bean>

	<bean id="dept" class="com.trent.bean.Dept">
		<property name="deptName" value="dd"></property>
		<property name="deptNo" value="dfdfff"></property>
	</bean>
</beans>